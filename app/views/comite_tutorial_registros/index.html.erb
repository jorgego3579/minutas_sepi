<p id="notice"><%= notice %></p>

<h1>Comite Tutorial Registros</h1>

<table>
  <thead>
    <tr>
      <th>Primer tutor</th>
      <th>Segundo tutor</th>
      <th>Tercer tutor</th>
      <th>Cuarto tutor</th>
      <th>Quinto tutor</th>
      <th>Caso</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comite_tutorial_registros.each do |comite_tutorial_registro| %>
      <tr>
        <td><%= comite_tutorial_registro.primer_tutor %></td>
        <td><%= comite_tutorial_registro.segundo_tutor %></td>
        <td><%= comite_tutorial_registro.tercer_tutor %></td>
        <td><%= comite_tutorial_registro.cuarto_tutor %></td>
        <td><%= comite_tutorial_registro.quinto_tutor %></td>
        <td><%= comite_tutorial_registro.caso_id %></td>
        <td><%= link_to 'Show', comite_tutorial_registro %></td>
        <td><%= link_to 'Edit', edit_comite_tutorial_registro_path(comite_tutorial_registro) %></td>
        <td><%= link_to 'Destroy', comite_tutorial_registro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comite Tutorial Registro', new_comite_tutorial_registro_path %>
