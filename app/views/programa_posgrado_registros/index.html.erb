<h1>Programa Posgrado Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @programa_posgrado_registros.each do |programa_posgrado_registro| %>
      <tr>
        <td><%= programa_posgrado_registro.nombre %></td>
        <td><%= link_to 'Show', programa_posgrado_registro %></td>
