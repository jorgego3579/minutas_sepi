<p id="notice"><%= notice %></p>

<h1>Asignatura Registros</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Clave</th>
      <th>Creditos</th>
      <th>Programa posgrado registro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asignatura_registros.each do |asignatura_registro| %>
      <tr>
        <td><%= asignatura_registro.nombre %></td>
        <td><%= asignatura_registro.clave %></td>
        <td><%= asignatura_registro.creditos %></td>
        <td><%= asignatura_registro.programa_posgrado_registro_id %></td>
        <td><%= link_to 'Show', asignatura_registro %></td>
        <td><%= link_to 'Edit', edit_asignatura_registro_path(asignatura_registro) %></td>
        <td><%= link_to 'Destroy', asignatura_registro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asignatura Registro', new_asignatura_registro_path %>
